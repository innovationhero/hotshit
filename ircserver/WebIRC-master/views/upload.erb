<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
            "http://www.w3.org/TR/html4/loose.dtd">

<html lang="en">
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <title>Host File</title>
  <style type="text/css" media="screen">
  
    form#file_form {
      margin: 30px 10px;
    }
    
    input#file_input {
      margin-top: 6px;
      float: left;
    }
    
    div#spinner {
      background-image: url("data:image/gif;base64,");
      width: 36px;
      height: 39px;
      float: right;
      display: none;
    }
    
    div#ok_button {
      margin-top: 6px;
      font-family: "Lucida Grande", Helvetica, "MS Trebuchet", Arial, sans-serif;
      font-size: 14px;
      background-image: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAAVCAAAAACiOdvYAAAACXBIWXMAAAsTAAALEwEAmpwYAAAAG0lEQVQI12P4wfSL6S8K/MP0D4hhrL9gNhADALXzFN9rCYLAAAAAAElFTkSuQmCC");
      background-repeat: repeat-x;
      background-color: rgb(182, 182, 182);
      border-bottom: 1px solid black;
      color: black;
      text-align: center;
      text-shadow: #EEE 0px 1px 1px;
      padding: 2px 0px;
      width: 100px;
      border: 1px solid #444;
      -moz-border-radius: 10px;
      -webkit-border-radius: 10px;
      float: right;
    }
    
    div#ok_button:hover {
      cursor: pointer;
      text-decoration: underline;
    }
    
    div#uploading_text {
      font-family: "Lucida Grande", Helvetica, "MS Trebuchet", Arial, sans-serif;
      margin: 11px;
      font-size: 14px;
      text-align: center;
      display: none;
      float: right;
    }
  </style>
  <script type="text/javascript" charset="utf-8">
    function do_upload(form_file) {
      document.getElementById("file_input").style.display = "none"
      document.getElementById("ok_button").style.display = "none"
      document.getElementById("uploading_text").style.display = "block"
      document.getElementById("spinner").style.display = "block"
      form_file.submit()
    }
  </script>
</head>
<body>
  <form id="file_form" name="form_upload" action="upload" enctype="multipart/form-data" method="post" accept-charset="utf-8">
    <input id="file_input" type="file" name="datafile">
    <div onclick="do_upload(this.parentNode)" id="ok_button">
      Upload...
    </div>
    <div id="spinner">
      
    </div>
    <div id="uploading_text">
      Uploading, please wait
    </div>
  </form>
</body>
</html>